pages = {460--70},
pmid = {19324922},
title = {{Hospital staff attributions of the causes of physician variation in end-of-life treatment intensity.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/19324922 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC2707029},
